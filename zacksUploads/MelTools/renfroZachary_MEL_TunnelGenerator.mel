// executes the command 10 times
    //$i is how long it is
    for( $i=0; $i<10; ++$i )
    		{
    			// selects the wall
    			
                select -r GEO_TunnelFront ;
    
                // duplicates the wall
                duplicate -rr;
                
    			// move the wall a bit in z (x,y,z ordering)
    			xform -translation 0 0 ($i*1);
    			
    		}